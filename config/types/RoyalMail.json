{
	"name": "RoyalMail",
	"display": "Royal Mail",
	"show": false,
	"cache": true,
	"target": [
		"royal",
		"royal-mail"
	],
	"generator": ".RoyalMailGenerator",
	"threads": 2,
	"priority": 8,
	"cost": {
		"basic": 1,
		"custom": 1
	},
	"checksum": false,
	"nonprinting": false,
	"example": "11212345612345678",
	"pattern": {
		"auto": "^[0-9]{1,32}$",
		"extended": "^[0-9]{1,32}$"
	},
	"description": {
		"en": "Royal Mail"
	},
	"wiki": {
		"en": "https://en.wikipedia.org/wiki/PostBar"
	},
	"options": {
		"dpi": {
			"name": "Resolution",
			"type": "number",
			"default": 180,
			"min": 50,
			"max": 300,
			"step": 10
		},
		"module": {
			"name": "Module",
			"type": "number",
			"default": 10,
			"min": 1,
			"max": 35,
			"step": 1
		},
		"qz": {
			"name": "Quiet Zone",
			"type": "number",
			"default": 2,
			"min": 0,
			"max": 20,
			"step": 1
		},
		"height": {
			"name": "Height",
			"type": "number",
			"default": 22,
			"min": 5,
			"max": 150,
			"step": 5
		},
		"font": {
			"name": "Font Size",
			"type": "number",
			"default": 4,
			"min": 0,
			"max": 20,
			"step": 2
		},
		"bg": {
			"name": "Background",
			"type": "text",
			"default": "ffffff",
			"pattern": "[0-9A-F]{6}"
		},
		"fg": {
			"name": "Foreground",
			"type": "text",
			"default": "000000",
			"pattern": "[0-9A-F]{6}"
		}
	}
}