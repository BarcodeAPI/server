{
	"name": "PDF417",
	"display": "PDF-417",
	"show": true,
	"decode": true,
	"cache": true,
	"target": [
		"417",
		"pdf417",
		"pdf"
	],
	"generator": ".PDF417Generator",
	"threads": 2,
	"priority": 45,
	"cost": {
		"basic": 2,
		"custom": 4
	},
	"checkdigit": 0,
	"nonprinting": true,
	"example": [
		"PDF - 417"
	],
	"pattern": {
		"auto": "^[ !\\\"#$%&\\'()*+,\\-.\\/0-9:;<=>?@A-Z\\[\\]^_`a-z{|}~]{1,2335}$",
		"extended": "^[ !\\\"#$%&\\'()*+,\\-.\\/0-9:;<=>?@A-Z\\[\\]^_`a-z{|}~]{1,2335}$"
	},
	"description": {
		"en": "A stacked linear barcode most commonly found on identification cards."
	},
	"wiki": {
		"en": "https://en.wikipedia.org/wiki/PDF417"
	},
	"options": {
		"dpi": {
			"name": "Resolution",
			"type": "number",
			"default": 150,
			"min": 50,
			"max": 300,
			"step": 10
		},
		"module": {
			"name": "Module",
			"type": "number",
			"default": 6,
			"min": 1,
			"max": 35,
			"step": 1
		},
		"qz": {
			"name": "Quiet Zone",
			"type": "number",
			"default": 4,
			"min": 0,
			"max": 20,
			"step": 1
		},
		"height": {
			"name": "Height",
			"type": "number",
			"default": 6,
			"min": 2,
			"max": 150,
			"step": 5
		},
		"bg": {
			"name": "Background",
			"type": "text",
			"default": "ffffff",
			"pattern": "[0-9A-F]{6}"
		},
		"fg": {
			"name": "Foreground",
			"type": "text",
			"default": "000000",
			"pattern": "[0-9A-F]{6}"
		}
	}
}