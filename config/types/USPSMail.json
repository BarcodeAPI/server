{
	"name": "USPSMail",
	"display": "USPS Mail",
	"show": true,
	"target": [
		"usps",
		"intelligent-mail"
	],
	"generator": ".USPSMailGenerator",
	"threads": 2,
	"priority": 10,
	"cost": {
		"basic": 1,
		"custom": 2
	},
	"checksum": false,
	"nonprinting": false,
	"example": "0123456709498765432101234567891",
	"pattern": {
		"auto": "^[0-9]{1,32}$",
		"extended": "^[0-9]{1,32}$"
	},
	"description": {
		"en": "Used by the United States Postal Service for Intelligent Mail sorting."
	},
	"wiki": {
		"en": "https://en.wikipedia.org/wiki/Intelligent_Mail_barcode"
	},
	"options": [
		{
			"name": "dpi",
			"type": "number",
			"default": 150,
			"min": 50,
			"max": 300
		},
		{
			"name": "bg",
			"type": "text",
			"default": "ffffff",
			"pattern": "[0-9A-F]{6}"
		},
		{
			"name": "fg",
			"type": "text",
			"default": "000000",
			"pattern": "[0-9A-F]{6}"
		}
	]
}