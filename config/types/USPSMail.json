{
	"name": "USPSMail",
	"display": "USPS Mail",
	"show": false,
	"decode": false,
	"cache": true,
	"target": [
		"usps",
		"uspsmail",
		"intelligent-mail"
	],
	"generator": ".USPSMailGenerator",
	"threads": 2,
	"priority": 10,
	"cost": {
		"basic": 1,
		"custom": 2
	},
	"checkdigit": 0,
	"nonprinting": false,
	"example": "0123456709498765432101234567891",
	"pattern": {
		"auto": "^[0-9]{1,32}$",
		"extended": "^[0-9]{1,32}$"
	},
	"description": {
		"en": "Used by the United States Postal Service for Intelligent Mail sorting."
	},
	"wiki": {
		"en": "https://en.wikipedia.org/wiki/Intelligent_Mail_barcode"
	},
	"options": {
		"dpi": {
			"name": "Resolution",
			"type": "number",
			"default": 180,
			"min": 50,
			"max": 300,
			"step": 10
		},
		"module": {
			"name": "Module",
			"type": "number",
			"default": 10,
			"min": 1,
			"max": 35,
			"step": 1
		},
		"qz": {
			"name": "Quiet Zone",
			"type": "number",
			"default": 2,
			"min": 0,
			"max": 20,
			"step": 1
		},
		"height": {
			"name": "Height",
			"type": "number",
			"default": 22,
			"min": 5,
			"max": 150,
			"step": 5
		},
		"font": {
			"name": "Font Size",
			"type": "number",
			"default": 4,
			"min": 0,
			"max": 20,
			"step": 2
		},
		"text": {
			"name": "Text Placement",
			"type": "option",
			"default": "bottom",
			"options": [
				"bottom",
				"top",
				"none"
			]
		},
		"bg": {
			"name": "Background",
			"type": "text",
			"default": "ffffff",
			"pattern": "[0-9A-F]{6}"
		},
		"fg": {
			"name": "Foreground",
			"type": "text",
			"default": "000000",
			"pattern": "[0-9A-F]{6}"
		}
	}
}