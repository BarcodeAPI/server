{
	"name": "UPC_E",
	"display": "UPC E",
	"show": true,
	"decode": true,
	"cache": true,
	"target": [
		"e",
		"upc-e",
		"upce"
	],
	"generator": ".UPCEGenerator",
	"threads": 2,
	"priority": 98,
	"cost": {
		"basic": 1,
		"custom": 3
	},
	"checkdigit": 8,
	"nonprinting": false,
	"example": "01023459",
	"pattern": {
		"auto": "^(0|1)[0-9]{7}$",
		"extended": "^(0|1)[0-9]{6,7}(\\+(([0-9]{2})|([0-9]{5}))){0,1}$"
	},
	"description": {
		"en": "A compact version of UPC-A, removing unneeded '0's."
	},
	"wiki": {
		"en": "https://en.wikipedia.org/wiki/Universal_Product_Code"
	},
	"options": {
		"dpi": {
			"name": "Resolution",
			"type": "number",
			"default": 150,
			"min": 50,
			"max": 300,
			"step": 10
		},
		"module": {
			"name": "Module",
			"type": "number",
			"default": 4,
			"min": 1,
			"max": 35,
			"step": 1
		},
		"qz": {
			"name": "Quiet Zone",
			"type": "number",
			"default": 4,
			"min": 0,
			"max": 20,
			"step": 1
		},
		"height": {
			"name": "Height",
			"type": "number",
			"default": 22,
			"min": 5,
			"max": 150,
			"step": 5
		},
		"font": {
			"name": "Font Size",
			"type": "number",
			"default": 5,
			"min": 0,
			"max": 20,
			"step": 2
		},
		"text": {
			"name": "Text Placement",
			"type": "option",
			"default": "bottom",
			"options": [
				"bottom",
				"top",
				"none"
			]
		},
		"pattern": {
			"name": "Pattern",
			"type": "text",
			"default": "_",
			"pattern": "[ _]+"
		},
		"bg": {
			"name": "Background",
			"type": "text",
			"default": "ffffff",
			"pattern": "[0-9A-F]{6}"
		},
		"fg": {
			"name": "Foreground",
			"type": "text",
			"default": "000000",
			"pattern": "[0-9A-F]{6}"
		}
	}
}
